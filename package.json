  "dependencies": {
    "axios": "^0.19.0",
    "concurrently": "^5.0.0",
    "express": "^4.18.2",
    "touch": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
}
