{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "boilerplate for react and express",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
